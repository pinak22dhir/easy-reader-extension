{
    "manifest_version": 3,
    "name": "Easy Reader",
    "description": "This extension helps read papers easily",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["handleSelection.js", "content.js", "handleFig.js", "nature/extractImageUrl.js", "nature/remove_trailing_slash.js", "nature/extractMethods.js", "handleMethods.js", "findSimilarMethods.js", "handleRef.js", "nature/extractRef.js", "getCrossRef.js", "scrapePubmed.js"]
        }
    ]
}
