{
    "manifest_version": 2,
    "name": "Easy Reader",
    "description": "This extension helps read papers easily",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["handleSelection.js", "content.js"]
        }
    ]
}
