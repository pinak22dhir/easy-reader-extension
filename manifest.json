{
    "manifest_version": 3,
    "name": "Easy Reader",
    "description": "This extension helps read papers easily",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["handleSelection.js", "content.js", "showFig.js", "nature/NatureScrapeImageUrl.js", "showMethods.js", "findSimilarMethods.js", "showRef.js", "getCrossRef.js", "scrapePubmed.js"]
        }
    ]
}
